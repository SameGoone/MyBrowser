<Window x:Class="MyBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyBrowser"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="687">
    <Grid>
        <TextBox x:Name="TextBox1" HorizontalAlignment="Left" Margin="10,10,0,34" TextWrapping="Wrap" Width="310" AcceptsReturn = "True"/>
        <Button Content="Parse" HorizontalAlignment="Left" Margin="241,0,0,9" VerticalAlignment="Bottom" Click="Button_Click" Width="79"/>
        <TreeView x:Name="treeView" Margin="0,10,10,34" HorizontalAlignment="Right" RenderTransformOrigin="0.5,0.5" Width="333">
            <TreeView.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TreeView.RenderTransform>
        </TreeView>
    </Grid>
</Window>
